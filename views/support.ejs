<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
    <link rel="stylesheet" href="/support.css">
    <title>Document</title>
</head>
<body>
    <h1 class="header-1 text-center">User Concerns</h1>
    <div class="container mt-5 pt-5">
        <table class="table">
            <tr>
                <th>Token number</th>
                <th>User ID</th>
                <th>User's concern</th>
                <th>Date</th>

            </tr>
           <% result.forEach(function(item) { %>
                <tr>
                    <td><%= item.token_number %></td>
                    <td><%= item.id %></td>
                    <td><%= item.concern %></td>
                    <td><%= item.date_time %></td>
                    <td>
                        <form action="/resolve" method="post">
                            <input type="hidden" value="<%= item.token_number %>" name="token_number">
                            <input type="submit" value="Resolve" class="btn btn-primary">
                        </form>
                    </td>
                </tr>

           <% }) %>
        </table>
        <form action="/admin_page" method="get">
            
            <input type="submit" value="Go Back" class="btn btn-primary">
        </form>
    </div>
    <script src="C:\bootstrap-5.2.3-dist\js\bootstrap.min.js"></script>
</body>
</html>